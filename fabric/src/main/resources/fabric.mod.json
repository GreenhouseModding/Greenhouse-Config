{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}+${minecraft_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    "${fabric_mod_contributors}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "issues": "https://github.com/${sources}/issues",
    "sources": "https://github.com/${sources}"
  },
  "license": "${mod_license}",
  "icon": "${mod_id}.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.greenhouseteam.greenhouseconfig.impl.GreenhouseConfigFabric"
    ],
    "client": [
      "dev.greenhouseteam.greenhouseconfig.impl.client.GreenhouseConfigFabricClient"
    ]
  },
  "accessWidener": "greenhouseconfig.accesswidener",
  "depends": {
    "fabricloader": "${fabric_loader_range}",
    "fabric-api": "*",
    "minecraft": "${fabric_minecraft_version_range}",
    "java": ">=${java_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/${sources}/releases"
      }
    }
  }
}